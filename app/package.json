{
  "name": "aicode-system",
  "version": "1.0.0",
  "description": "Automated AI-powered code processing and management system with flow builders",
  "main": "core/aicode-engine.ts",
  "scripts": {
    "start": "ts-node core/aicode-engine.ts",
    "dev": "nodemon --exec ts-node core/aicode-engine.ts",
    "api": "ts-node api-server.ts",
    "api:dev": "nodemon --exec ts-node api-server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "aicode:auto": "ts-node core/aicode-engine.ts auto .",
    "aicode:analyze": "ts-node core/aicode-engine.ts analyze .",
    "aicode:generate": "ts-node core/aicode-engine.ts generate .",
    "sync": "ts-node utils/auto-sync.ts .",
    "config": "ts-node utils/auto-config.ts .",
    "repair": "ts-node utils/auto-repair.ts .",
    "fix": "ts-node utils/auto-fix.ts .",
    "auto-test": "ts-node utils/auto-test.ts ./src",
    "deploy": "ts-node utils/auto-deploy.ts vercel production",
    "comments": "ts-node utils/auto-comments.ts ./src",
    "sync:components": "ts-node utils/component-auto-sync.ts app,web,mobile .",
    "sync:components:watch": "ts-node utils/component-auto-sync.ts app,web,mobile . --watch",
    "sync:components:db": "ts-node utils/component-auto-sync.ts app,web,mobile . --database",
    "config:components": "ts-node utils/component-auto-config.ts app,web,mobile . postgres",
    "config:components:mongo": "ts-node utils/component-auto-config.ts app,web,mobile . mongodb"
  },
  "keywords": [
    "aicode",
    "automation",
    "code-generation",
    "flow-builder",
    "testing",
    "deployment"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
