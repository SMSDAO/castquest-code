{
  "version": 2,
  "name": "aicode-admin-ui",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@aicode-api-url",
      "NEXT_PUBLIC_WS_URL": "@aicode-ws-url"
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}
